// checks if 'x' and 'o' occur the same number of times in the string
function ExOh(str) {
  var countX = 0;
  var countO = 0;
  for (var i = 0; i < str.length; i++) {
    if(str[i] === 'x') countX++;
  }
  for (var i = 0; i < str.length; i++) {
    if(str[i] === 'o') countO++;
  }
  if (countX === countO) return true;
  else return false;
}
